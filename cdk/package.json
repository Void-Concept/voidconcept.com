{
    "name": "@voidconcept/cdk",
    "version": "0.1.0",
    "bin": {
        "cdk": "bin/cdk.js"
    },
    "scripts": {
        "build": "tsc",
        "build-deps": "yarn --cwd '../lambdas/globalStorage' bundle && yarn --cwd '../lambdas/runescapeProxy' bundle && yarn --cwd '../lambdas/calendar' bundle && yarn --cwd '../lambdas/questList' bundle && yarn --cwd '../lambdas/notes' bundle",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "cdk-diff": "yarn build-deps && cdk diff",
        "cdk-diff-nobuild": "cdk diff",
        "cdk-deploy": "yarn build-deps && cdk deploy \"*\"",
        "cdk-deploy-nobuild": "cdk deploy \"*\""
    },
    "devDependencies": {
        "@types/jest": "^27.0.3",
        "@types/node": "16.11.10",
        "aws-cdk": "^2.24.1",
        "jest": "^27.3.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.7.0",
        "typescript": "~4.5.2"
    },
    "dependencies": {
        "aws-cdk-lib": "^2.24.1",
        "constructs": "^10.1.7",
        "source-map-support": "^0.5.21"
    }
}
