{
    "name": "cdk",
    "version": "0.1.0",
    "bin": {
        "cdk": "bin/cdk.js"
    },
    "scripts": {
        "build": "tsc",
        "build-deps": "yarn --cwd '../globalStorageLambda' bundle",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "cdk-diff": "yarn build-deps && cdk diff",
        "cdk-deploy": "yarn build-deps && cdk deploy \"*\""
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.70.0",
        "@types/jest": "^26.0.15",
        "@types/node": "14.14.2",
        "aws-cdk": "1.70.0",
        "jest": "^26.6.1",
        "ts-jest": "^26.4.2",
        "ts-node": "^9.0.0",
        "typescript": "~4.0.3"
    },
    "dependencies": {
        "@aws-cdk/aws-apigateway": "^1.70.0",
        "@aws-cdk/aws-certificatemanager": "^1.70.0",
        "@aws-cdk/aws-cloudfront": "^1.70.0",
        "@aws-cdk/aws-cognito": "^1.70.0",
        "@aws-cdk/aws-dynamodb": "^1.70.0",
        "@aws-cdk/aws-lambda": "^1.70.0",
        "@aws-cdk/aws-route53": "^1.70.0",
        "@aws-cdk/aws-route53-targets": "^1.70.0",
        "@aws-cdk/aws-s3": "^1.70.0",
        "@aws-cdk/core": "1.70.0",
        "source-map-support": "^0.5.19"
    }
}
