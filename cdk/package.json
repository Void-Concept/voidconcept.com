{
    "name": "cdk",
    "version": "0.1.0",
    "bin": {
        "cdk": "bin/cdk.js"
    },
    "scripts": {
        "build": "tsc",
        "build-deps": "yarn --cwd '../globalStorageLambda' bundle && yarn --cwd '../runescapeProxyLambda' bundle && yarn --cwd '../calendarLambda' bundle && yarn --cwd '../questListLambda' bundle && yarn --cwd '../notesLambda' bundle",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "cdk-diff": "yarn build-deps && cdk diff",
        "cdk-diff-nobuild": "cdk diff",
        "cdk-deploy": "yarn build-deps && cdk deploy \"*\"",
        "cdk-deploy-nobuild": "cdk deploy \"*\""
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.89.0",
        "@types/jest": "^26.0.20",
        "@types/node": "14.14.28",
        "aws-cdk": "1.89.0",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.1",
        "ts-node": "^9.1.1",
        "typescript": "~4.1.5"
    },
    "dependencies": {
        "@aws-cdk/aws-apigateway": "^1.89.0",
        "@aws-cdk/aws-certificatemanager": "^1.89.0",
        "@aws-cdk/aws-cloudfront": "^1.89.0",
        "@aws-cdk/aws-cognito": "^1.89.0",
        "@aws-cdk/aws-dynamodb": "^1.89.0",
        "@aws-cdk/aws-lambda": "^1.89.0",
        "@aws-cdk/aws-route53": "^1.89.0",
        "@aws-cdk/aws-route53-targets": "^1.89.0",
        "@aws-cdk/aws-s3": "^1.89.0",
        "@aws-cdk/aws-secretsmanager": "1.89.0",
        "@aws-cdk/core": "1.89.0",
        "source-map-support": "^0.5.19"
    }
}