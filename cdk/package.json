{
    "name": "cdk",
    "version": "0.1.0",
    "bin": {
        "cdk": "bin/cdk.js"
    },
    "scripts": {
        "build": "tsc",
        "build-deps": "yarn --cwd '../globalStorageLambda' bundle && yarn --cwd '../runescapeProxyLambda' bundle && yarn --cwd '../calendarLambda' bundle && yarn --cwd '../questListLambda' bundle && yarn --cwd '../notesLambda' bundle",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "cdk-diff": "yarn build-deps && cdk diff",
        "cdk-diff-nobuild": "cdk diff",
        "cdk-deploy": "yarn build-deps && cdk deploy \"*\"",
        "cdk-deploy-nobuild": "cdk deploy \"*\""
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.140.0",
        "@types/jest": "^27.0.3",
        "@types/node": "16.11.10",
        "aws-cdk": "1.140.0",
        "jest": "^27.3.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "~4.5.2"
    },
    "dependencies": {
        "@aws-cdk/aws-apigateway": "1.140.0",
        "@aws-cdk/aws-certificatemanager": "1.140.0",
        "@aws-cdk/aws-cloudfront": "1.140.0",
        "@aws-cdk/aws-cognito": "1.140.0",
        "@aws-cdk/aws-dynamodb": "1.140.0",
        "@aws-cdk/aws-iam": "1.140.0",
        "@aws-cdk/aws-lambda": "1.140.0",
        "@aws-cdk/aws-route53": "1.140.0",
        "@aws-cdk/aws-route53-targets": "1.140.0",
        "@aws-cdk/aws-s3": "1.140.0",
        "@aws-cdk/aws-secretsmanager": "1.140.0",
        "@aws-cdk/core": "1.140.0",
        "source-map-support": "^0.5.21"
    }
}
